{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": ["airbnb/base"],
    "rules": {
        "no-plusplus": "off",
        "eol-last": "off",
        "max-len": ["error", 150, { "ignoreUrls": true}],
        "no-tabs": "off",
        "no-alert": "off",
        "no-multiple-empty-lines": "off",
        "prefer-destructuring": "off",
        "indent": [0, "tab"]

    }
}